<p id="alert"><%= alert %></p>
<p id="notice"><%= notice %></p>
<h1>Вход в систему</h1>
<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
    <p></p>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <p></p>
  </div>
  <div class="actions">
    <%= submit_tag "Login" %>
    <%= link_to "Sign Up", signup_path %>
  </div>
<% end %>